<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Fanisko Url 2 edit</title>
    <link rel="stylesheet" href="https://codepen.io/gymratpacks/pen/VKzBEp#0" />
    <link
      href="https://fonts.googleapis.com/css?family=Nunito:400,300"
      rel="stylesheet"
      type="text/css"
    />
    <link rel="stylesheet" href="/Edit2.css" />
  </head>
  <body>
    <div class="row">
      <div class="col-md-12">
        <form action="/fanisko2" method="POST">
          <h1>Edit Match 2 Details</h1>

          <fieldset>
            <legend><span class="number"></span> Match 2 Details</legend>
            <label for="stockNum">
              <h3>
                Current ID :
                <b
                  ><i><h4><%=id%></h4></i></b
                >
              </h3></label
            >
            <input
              type="text"
              id="MatchIdOld"
              name="matchIdOld"
              readonly
              value="<%=id%>"
            />
            <label for="stockNum">Match Id:</label>
            <input type="text" id="MatchId" name="matchId" />
          </fieldset>

          <button type="submit">Edit</button>
        </form>
      </div>
    </div>
  </body>
</html>
